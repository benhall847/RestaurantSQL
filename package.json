{
  "name": "node",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon app.js",
    "db:create": "createdb restaurant-app",
    "db:init": "psql -f schema.sql restaurant-app && psql -f seed.sql restaurant-app",
    "db:destroy": "dropdb restaurant-app",
    "db:reset": "npm run db:destroy && npm run db:create && npm run db:init",
    "test": "mocha",
    "test:watch": "mocha -w"
  },
